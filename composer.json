{
    "name": "maesepp/braintree",
    "description": "A Braintree bridge for Laravel",
    "keywords": ["laravel", "framework", "Laravel-Braintree", "Laravel Braintree", "Tarvo Mäesepp", "MaeseppTarvo", "maesepp"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tarvo Mäesepp",
            "email": "tarvomaesepp@gmail.com",
            "homepage": "http://maesepp.com"
        }],
        "require": {
            "php": "^5.5.9 || ^7.0",
            "illuminate/support": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.*",
            "graham-campbell/manager": "^2.4",
            "braintree/braintree_php": "^3.25"
        },
        "require-dev": {
            "graham-campbell/testbench": "^3.3",
            "mockery/mockery": "0.9.*",
            "phpunit/phpunit": "~4.8 || ~6.0"
        },
        "autoload": {
        "psr-4": {
            "Maesepp\\Braintree\\": "src/"
            }
        },
        "laravel": {
            "providers": [
                "Maesepp\\Braintree\\BraintreeServiceProvider"
            ],
            "aliases": {
                "Braintree": "Maesepp\\Braintree\\Facades\\Braintree"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
    
}
